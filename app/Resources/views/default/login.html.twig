{# app/Resources/views/security/login.html.twig #}
{% extends 'base.html.twig' %}

{% form_theme form 'form/FormMaterializeCssTheme.html.twig' %}


{% block body %}

    {{  form_start(form) }}
        {{  form_errors(form) }}

        {{ form_row(form.username) }}
        {{ form_row(form.password) }}
        {{ form_row(form.submit, { 'label': 'Login' }) }}

    {{ form_end(form) }}

{% endblock %}


